@page
@model ResultsModel
@{
    ViewData[ViewConstants.Title] = Model.Title;
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        @AppResources.Results
        <small>@AppResources.ResultsDescription</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/"><i class="fa fa-dashboard"></i>@AppResources.Home</a></li>
        <li class="active">@AppResources.Results</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Project: Glut - Run: 1</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="resultsTable" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>@AppResources.Start</th>
                                <th>@AppResources.End</th>
                                <th>@AppResources.Url</th>
                                <th>@AppResources.Success</th>
                                <th>@AppResources.StatusCode</th>
                                <th>@AppResources.HeaderLength</th>
                                <th>@AppResources.ResponseLength</th>
                                <th>@AppResources.TotalLength</th>
                                <th>@AppResources.RequestMs</th>
                                <th>@AppResources.ResponseMs</>
                                <th>@AppResources.TotalLength</th>
                                <th>@AppResources.ResponseHeaders</th>
                                <th>@AppResources.Exception</th>
                                <th>@AppResources.CreatedDateTime</th>
                                <th>@AppResources.CreatedByUser</th>
                            </tr>
                        </thead>
                        <tbody>
                        <tfoot>
                            <tr>
                                <th>Rendering engine</th>
                                <th>Browser</th>
                                <th>Platform(s)</th>
                                <th>Engine version</th>
                                <th>CSS grade</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->


@section Scripts
{
    <script>
        $(function ()
        {
            $('#resultsTable').DataTable();
        });
    </script>
}
