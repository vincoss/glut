<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Xamarin_Samples.Views.AppInformationView"
             Title="AppInformationView">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="App Information" />
            <ListView x:Name="lstInfo"/>
            <Label Text="Show App Settings" />
            <Button x:Name="btnShow" Text="Show" Clicked="btnShow_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>