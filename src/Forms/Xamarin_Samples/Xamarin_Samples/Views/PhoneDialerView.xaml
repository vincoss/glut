<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Xamarin_Samples.Views.PhoneDialerView"
             Title="PhoneDialerView">

    <!--
    https://docs.microsoft.com/en-us/xamarin/essentials/phone-dialer?context=xamarin/xamarin-forms
    -->
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="PhoneDialer - Sample"/>
            <Button x:Name="btnShow" Text="Open" Clicked="btnShow_Clicked"/>
            <Label x:Name="lblError" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>